{
  "name": "hifide",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && pnpm -s prepackage:kill && pnpm -s rebuild:native && pnpm -s package:electron",
    "prepackage:kill": "powershell -NoProfile -Command \"try{Get-Process HiFide -ErrorAction SilentlyContinue|Stop-Process -Force}catch{}; try{Get-Process electron -ErrorAction SilentlyContinue|Stop-Process -Force}catch{}; Start-Sleep -Milliseconds 300\"",
    "rebuild:native": "pnpm exec electron-rebuild -f -w node-pty",
    "clean:release": "node --input-type=module -e \"import fs from 'fs'; try{fs.rmSync('release',{recursive:true,force:true})}catch(e){}\"",
    "package:electron": "powershell -NoProfile -Command \"$ts=(Get-Date -Format 'yyyyMMdd-HHmmss'); $ver=$env:npm_package_version; $out=\\\"release/$ver-$ts\\\"; electron-builder --config.directories.output=\\\"$out\\\"\"",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:record": "cross-env TEST_MODE=record jest",
    "test:live": "cross-env TEST_MODE=live jest",
    "postinstall": "node scripts/postinstall.cjs"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.65.0",
    "@ast-grep/lang-c": "^0.0.3",
    "@ast-grep/lang-cpp": "^0.0.3",
    "@ast-grep/lang-csharp": "^0.0.3",
    "@ast-grep/lang-go": "^0.0.3",
    "@ast-grep/lang-java": "^0.0.4",
    "@ast-grep/lang-kotlin": "^0.0.4",
    "@ast-grep/lang-php": "^0.0.4",
    "@ast-grep/lang-python": "^0.0.3",
    "@ast-grep/lang-ruby": "^0.0.4",
    "@ast-grep/lang-swift": "^0.0.5",
    "@ast-grep/napi": "^0.39.6",
    "@emotion/react": "^11.14.0",
    "@google/genai": "^1.25.0",
    "@mantine/core": "^8.3.3",
    "@mantine/hooks": "^8.3.3",
    "@mantine/notifications": "^8.3.3",
    "@monaco-editor/react": "^4.7.0",
    "@tanstack/react-query": "^5.90.2",
    "@types/dagre": "^0.7.53",
    "@uiw/react-json-view": "2.0.0-alpha.39",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "@zubridge/electron": "^2.0.0",
    "ajv": "^8.17.1",
    "conf": "^15.0.2",
    "dagre": "^0.8.5",
    "dequal": "^2.0.3",
    "dompurify": "^3.2.7",
    "electron-store": "^11.0.2",
    "fast-glob": "^3.3.3",
    "fastembed": "^2.0.0",
    "highlight.js": "^11.11.1",
    "ignore": "^5.3.2",
    "markdown-it": "^14.1.0",
    "monaco-editor": "^0.54.0",
    "node-pty": "^1.0.0",
    "openai": "^6.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reactflow": "^11.11.4",
    "ts-morph": "^27.0.0",
    "ws": "^8.18.3",
    "zod": "^4",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@jest/globals": "^30.2.0",
    "@tabler/icons-react": "^3.35.0",
    "@types/jest": "^30.0.0",
    "@types/markdown-it": "^14.1.2",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "cross-env": "^10.1.0",
    "dotenv-cli": "^10.0.0",
    "electron": "^38.2.1",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest": "^30.2.0",
    "node-gyp": "^10.3.1",
    "patch-package": "^8.0.1",
    "ts-jest": "^29.4.5",
    "typescript": "^5",
    "vite": "^5.1.6",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "main": "dist-electron/main.mjs",
  "build": {
    "asar": true,
    "asarUnpack": [
      "**/@ast-grep/napi/**/*.{node,dll,so,dylib}",
      "**/@ast-grep/lang-*/**/*.{dll,so,dylib}",
      "**/onnxruntime-node/**/*.{node,dll,so,dylib}",
      "**/node-pty/**/*.{node,dll,exe}",
      "node_modules/node-pty/build/Release/*.{node,dll,exe}"
    ],
    "files": [
      "dist/**",
      "dist-electron/**",
      "node_modules/**",
      "package.json",
      "!release/**",
      "!scripts/**",
      "!**/__tests__/**",
      "!**/*.map",
      "!src/**"
    ],
    "npmRebuild": false,
    "icon": "public/hifide-logo.png",
    "mac": {
      "icon": "public/hifide-logo.png"
    },
    "win": {
      "icon": "public/hifide-logo.png"
    },
    "linux": {
      "icon": "public/hifide-logo.png"
    }
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "keytar",
      "node-pty"
    ],
    "public-hoist-pattern": [
      "*"
    ],
    "ignoredBuiltDependencies": [],
    "patchedDependencies": {
      "node-pty@1.0.0": "patches/node-pty@1.0.0.patch"
    }
  }
}
