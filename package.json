{
  "name": "hifide",
  "version": "0.0.0",
  "type": "module",
  "author": "Joe Leaver",
  "homepage": "https://github.com/joeleaver/hifide",
  "repository": {
    "type": "git",
    "url": "https://github.com/joeleaver/hifide"
  },
  "description": "Agentic-first IDE",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && pnpm -s prepackage:kill && pnpm -s rebuild:native && pnpm -s package:electron",
    "prepackage:kill": "node scripts/prepackage-kill.cjs",
    "rebuild:native": "pnpm exec electron-rebuild -f -w node-pty,sharp",
    "clean:release": "node --input-type=module -e \"import fs from 'fs'; try{fs.rmSync('release',{recursive:true,force:true})}catch(e){}\"",
    "package:electron": "node scripts/package-electron.cjs",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:record": "cross-env TEST_MODE=record jest",
    "test:live": "cross-env TEST_MODE=live jest",
    "postinstall": "node scripts/postinstall.cjs"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.39",
    "@ai-sdk/fireworks": "^1.0.25",
    "@ai-sdk/google": "^2.0.25",
    "@ai-sdk/openai": "^2.0.57",
    "@ai-sdk/xai": "^2.0.30",
    "@anthropic-ai/sdk": "^0.65.0",
    "@dqbd/tiktoken": "^1.0.22",
    "@emotion/react": "^11.14.0",
    "@google/genai": "^1.25.0",
    "@hello-pangea/dnd": "^16.5.0",
    "@mantine/core": "^8.3.6",
    "@mantine/hooks": "8.3.6",
    "@mantine/notifications": "^8.3.6",
    "@mdxeditor/editor": "^3.48.0",
    "@monaco-editor/react": "^4.7.0",
    "@tanstack/react-query": "^5.90.6",
    "@uiw/react-json-view": "2.0.0-alpha.39",
    "@xenova/transformers": "^2.17.2",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "ai": "^5.0.87",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "conf": "^15.0.2",
    "dagre": "^0.8.5",
    "dequal": "^2.0.3",
    "dompurify": "^3.3.0",
    "electron-log": "^5.4.3",
    "electron-store": "^11.0.2",
    "fast-glob": "^3.3.3",
    "highlight.js": "^11.11.1",
    "ignore": "^5.3.2",
    "json-rpc-2.0": "^1.7.1",
    "markdown-it": "^14.1.0",
    "monaco-editor": "^0.54.0",
    "node-pty": "^1.0.0",
    "openai": "^6.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reactflow": "^11.11.4",
    "ts-morph": "^27.0.0",
    "vscode-ripgrep": "^1.13.2",
    "ws": "^8.18.3",
    "zod": "^4"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@jest/globals": "^30.2.0",
    "@tabler/icons-react": "^3.35.0",
    "@types/dagre": "^0.7.53",
    "@types/dompurify": "^3.2.0",
    "@types/jest": "^30.0.0",
    "@types/markdown-it": "^14.1.2",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "cross-env": "^10.1.0",
    "dotenv-cli": "^10.0.0",
    "electron": "^38.2.1",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest": "^30.2.0",
    "node-gyp": "^10.3.1",
    "patch-package": "^8.0.1",
    "ts-jest": "^29.4.5",
    "typescript": "^5",
    "vite": "^5.1.6",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "main": "dist-electron/main.mjs",
  "build": {
    "asar": true,
    "asarUnpack": [
      "**/onnxruntime-node/**/*.{node,dll,so,dylib}",
      "**/node-pty/**/*.{node,dll,exe}",
      "node_modules/node-pty/build/Release/*.{node,dll,exe}",
      "**/vscode-ripgrep/**",
      "**/sharp/**/*.{node,dll,so,dylib}"
    ],
    "files": [
      "dist/**",
      "dist-electron/**",
      "node_modules/**",
      "package.json",
      "!release/**",
      "!scripts/**",
      "!**/__tests__/**",
      "!**/*.map",
      "!src/**"
    ],
    "beforePack": "scripts/beforePack-rebuild-pty.cjs",
    "afterPack": "scripts/afterPack-assert-pty.cjs",
    "npmRebuild": false,
    "icon": "public/hifide-logo.png",
    "mac": {
      "icon": "public/hifide-logo.png"
    },
    "win": {
      "icon": "public/hifide-logo.png"
    },
    "linux": {
      "icon": "public/hifide-logo.png"
    }
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "keytar",
      "protobufjs",
      "sharp",
      "vscode-ripgrep"
    ],
    "public-hoist-pattern": [
      "*"
    ],
    "ignoredBuiltDependencies": [],
    "patchedDependencies": {
      "node-pty@1.0.0": "patches/node-pty@1.0.0.patch"
    }
  }
}
